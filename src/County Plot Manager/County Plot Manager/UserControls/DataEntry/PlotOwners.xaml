<UserControl x:Class="PlotManager.UI.UserControls.DataEntry.PlotOwners"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PlotManager.UI.UserControls.DataEntry"
             mc:Ignorable="d">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto"/>
			<RowDefinition Height="Auto"/>
		</Grid.RowDefinitions>
		<TextBlock Grid.Row="0" Text="Tenant" FontSize="20" Margin="2,-3,0,0" 
				Foreground="{StaticResource brush_Base_75}"/>
		<local:ContactInfo Grid.Row="1"/>
	</Grid>
</UserControl>
